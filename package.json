{
  "name": "roffline",
  "version": "0.1.0",
  "description": "Roffline allows you to browse Reddit offline",
  "main": "index.js",
  "scripts": {
    "update": "ncu --interactive --target minor",
    "task": "node -r ./env-checker.cjs ./TasksFile.js",
    "dev": "NODE_ENV=development LOGGING_LEVEL=debug node -r ./env-checker.cjs ./TasksFile.js dev:start",
    "inspect": "NODE_ENV=development LOGGING_LEVEL=debug node -r ./env-checker.cjs ./TasksFile.js dev:inspect",
    "test": "node ./TasksFile.js testAll",
    "build": "node ./TasksFile.js buildProd",
    "eslint": "node ./TasksFile.js tests:eslint",
    "updateTypes": "ts-typie",
    "start": "npm run build && NODE_ENV=production LOGGING_LEVEL=error PUBLIC_FOLDER=frontend-build node -r ./env-checker.cjs ./boot.js"
  },
  "browser": {
    "vue": "vue/dist/vue.esm-bundler.js"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 3 Chrome version",
    "last 2 safari version",
    "last 2 ios version"
  ],
  "pre-commit": [],
