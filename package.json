{
  "name": "roffline",
  "version": "0.1.0",
  "description": "Roffline allows you to browse Reddit offline",
  "main": "index.js",
  "scripts": {
    "update": "ncu --interactive --target minor",
    "task": "ts-node -r ./env-checker.cjs ./TasksFile.ts",
    "dev": "NODE_ENV=development ts-node -r ./env-checker.cjs ./TasksFile.ts dev:start",
    "inspect": "NODE_ENV=development ts-node -r ./env-checker.cjs ./TasksFile.ts dev:inspect",
    "test": "NODE_ENV=production ts-node ./TasksFile.ts testAll",
    "test-no-slow": "SKIP_SLOW_TESTS=true ts-node ./TasksFile.ts testAll",
    "build": "NODE_ENV=production ts-node ./TasksFile.ts buildProd",
    "eslint": "ts-node ./TasksFile.ts tests:eslint",
    "start": "npm run build && NODE_ENV=production node -r ./env-checker.cjs ./boot.js"
  },
  "browser": {
    "vue": "vue/dist/vue.esm-bundler.js",
    "vue3-json-viewer": "vue3-json-viewer/dist/bundle.cjs.js"
  },
  "engines": {
    "node": ">=16.13.0"
