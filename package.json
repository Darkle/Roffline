    "generate-passphrase": "^1.2.0",
    "got": "^11.8.2",
    "gridjs": "^5.0.2",
    "http-status-codes": "^2.1.4",
    "lazy-object-view": "^1.2.1",
    "lmdb": "^1.6.8",
    "lmdb-store": "^1.6.8",
