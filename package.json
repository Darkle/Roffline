{
  "name": "roffline",
  "version": "0.1.0",
  "description": "Roffline allows you to browse Reddit offline",
  "main": "index.js",
  "scripts": {
    "update": "ncu --interactive --target minor",
    "task": "node -r dotenv/config ./TasksFile.js",
    "dev": "NODE_ENV=development LOGGING_LEVEL=debug node -r dotenv/config ./TasksFile.js dev:start",
    "inspect": "NODE_ENV=development LOGGING_LEVEL=debug node -r dotenv/config ./TasksFile.js dev:inspect",
    "test": "node ./TasksFile.js testAll",
    "build": "node ./TasksFile.js buildProd",
    "eslint": "node ./TasksFile.js tests:eslint",
    "updateTypes": "ts-typie",
    "start": "npm run buildProd && NODE_ENV=production LOGGING_LEVEL=error PUBLIC_FOLDER=frontend-build node -r dotenv/config ./boot.js"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "browserslist": [
    "Firefox ESR",
    "last 3 Chrome version",
    "last 2 safari version",
    "last 2 ios version"
  ],
  "pre-commit": [],
  "dependencies": {
    "dotenv": "^10.0.0",
    "envalid": "^7.2.1",
